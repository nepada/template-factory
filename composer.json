{
    "name": "nepada/template-factory",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Latte powered template factory on steroids.",
    "keywords": ["nette", "latte", "template"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/template-factory/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "nette/utils": "^3.0@dev",
        "latte/latte": "^2.5.3",
        "nette/application": "^3.0@dev"
    },
    "require-dev": {
        "nette/tester": "^2.3.1",
        "mockery/mockery": "^1.2.4",
        "xificurk/phpstan-shim": "^0.11.8",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "nepada/coding-standard": "^6.3.0",
        "nette/bootstrap": "^3.0@dev",
        "nette/di": "^3.0.1@dev",
        "nette/http": "^3.0@dev"
    },
    "suggest": {
        "nette/di": "for integration with Nette DI container"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\Bridges\\": "src/Bridges/",
            "Nepada\\TemplateFactory\\": "src/TemplateFactory/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
